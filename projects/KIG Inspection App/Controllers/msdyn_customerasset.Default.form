    </FormTab>
    <FormTab xsi:type="AssociatedTab" flexiHeight="5" DocCommands="0">
      <Name>Inspection Templates</Name>
      <CustomName>Inspection Templates</CustomName>
      <Binding SchemaName="resco_questionnaire.@virtual"/>
      <Views>&lt;extra&gt;&lt;views&gt;resco_questionnaire.@Associated Questionnaires&lt;/views&gt;&lt;filter entity="resco_questionnaire"&gt;&amp;lt;fetch version="1.0"&amp;gt;&amp;lt;entity name="resco_questionnaire"&amp;gt;&amp;lt;filter type="and"&amp;gt;&amp;lt;condition attribute="resco_istemplate" operator="eq" value="1"/&amp;gt;&amp;lt;condition attribute="msdyn_customerassetcategoryid" entityname="L0" operator="eq" value="{{msdyn_customerassetcategory}}"/&amp;gt;&amp;lt;/filter&amp;gt;&amp;lt;link-entity name="btc_msdyn_customerassetcategory_resco_quest" alias="L0" from="resco_questionnaireid" to="resco_questionnaireid" link-type="outer"&amp;gt;&amp;lt;link-entity name="msdyn_customerassetcategory" alias="L1" from="msdyn_customerassetcategoryid" to="msdyn_customerassetcategoryid" link-type="inner"/&amp;gt;&amp;lt;/link-entity&amp;gt;&amp;lt;/entity&amp;gt;&amp;lt;/fetch&amp;gt;&lt;/filter&gt;&lt;/extra&gt;</Views>
      <LookupViews>&lt;extra&gt;&lt;views&gt;resco_questionnaire.Questionnaire Templates Lookup:resco_questionnaire.Reuse:resco_questionnaire.Staged Templates:resco_questionnaire.Templates&lt;/views&gt;&lt;filter entity="resco_questionnaire"&gt;&amp;lt;fetch&amp;gt;&amp;lt;entity name='resco_questionnaire'&amp;gt;&amp;lt;filter&amp;gt;&amp;lt;condition attribute='resco_istemplate' operator='eq' value='1' /&amp;gt;&amp;lt;/filter&amp;gt;&amp;lt;/entity&amp;gt;&amp;lt;/fetch&amp;gt;&lt;/filter&gt;&lt;/extra&gt;</LookupViews>
    </FormTab>
    <FormTab xsi:type="IFrameTab">
      <Name>Customer Asset</Name>
      <Url>file://CustomerAsset/CustomerAsset.html</Url>
      <IsVisible>false</IsVisible>
      <DelayLoad>false</DelayLoad>
    </FormTab>
    <FormTab xsi:type="AssociatedTab" DocCommands="0">
      <Name>Inspections</Name>
      <CustomName>Inspections</CustomName>
      <Binding SchemaName="resco_questionnaire.@virtual"/>
      <Views>&lt;extra&gt;&lt;views&gt;resco_questionnaire.Answered Questionnaires:resco_questionnaire.@In progress or completed:resco_questionnaire.Reuse:resco_questionnaire.Staged Templates:resco_questionnaire.Templates&lt;/views&gt;&lt;filter entity="resco_questionnaire"&gt;&amp;lt;fetch&amp;gt;&amp;lt;entity name="resco_questionnaire"&amp;gt;	&amp;lt;filter type="and"&amp;gt;		&amp;lt;condition attribute='resco_istemplate' operator='eq' value='0' /&amp;gt;		&amp;lt;filter type="or"&amp;gt;			&amp;lt;condition attribute="resco_regardingid" operator="eq" value="{{msdyn_customerassetid}}"/&amp;gt;			&amp;lt;condition attribute="resco_rawidvalue" entityname="QuestionLinkAlias" operator="eq" value="{{msdyn_customerassetid}}"/&amp;gt;		&amp;lt;/filter&amp;gt;	&amp;lt;/filter&amp;gt;&amp;lt;link-entity name="resco_question" alias="QuestionLinkAlias" from="resco_questionnaireid" to="resco_questionnaireid" link-type="outer"/&amp;gt;&amp;lt;/entity&amp;gt;&amp;lt;/fetch&amp;gt;&lt;/filter&gt;&lt;/extra&gt;</Views>
      <LookupViews>&lt;extra&gt;&lt;views&gt;resco_questionnaire.Questionnaire Templates Lookup:resco_questionnaire.Reuse:resco_questionnaire.Staged Templates&lt;/views&gt;&lt;filter entity="resco_questionnaire"&gt;&amp;lt;fetch&amp;gt;&amp;lt;entity name='resco_questionnaire'&amp;gt;&amp;lt;filter&amp;gt;&amp;lt;condition attribute='resco_istemplate' operator='eq' value='1' /&amp;gt;&amp;lt;/filter&amp;gt;&amp;lt;/entity&amp;gt;&amp;lt;/fetch&amp;gt;&lt;/filter&gt;&lt;/extra&gt;</LookupViews>
    </FormTab>
    <FormTab xsi:type="AssociatedTab" DocCommands="0">
      <Name>Sub Assets</Name>
      <CustomName>Sub Assets</CustomName>
      <Binding SchemaName="msdyn_customerasset.msdyn_masterasset"/>
      <Views>msdyn_customerasset.Default</Views>
    </FormTab>
    <FormTab xsi:type="AssociatedTab">
      <Name>Work Orders List</Name>
      <Binding SchemaName="msdyn_workorder.msdyn_customerasset"/>
    </FormTab>
    <FormTab xsi:type="AssociatedTab">
      <Name>Notes List</Name>
      <Binding SchemaName="annotation.objectid"/>
    </FormTab>
    <FormTab xsi:type="AssociatedTab">
      <Name>Activity List</Name>
      <Binding SchemaName="activity.regardingobjectid"/>
    </FormTab>
  </SerializedChildrenV2>
</EntityForm>