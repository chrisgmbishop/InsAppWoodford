<EntityForm xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:dummy="0" flexi="0" fleximode="1" fullscreen="0" resize="0" version="2">
  <IsContactForm>false</IsContactForm>
  <EntityName>msdyn_customerasset</EntityName>
  <CamelCase>msdyn_customerasset</CamelCase>
  <SharedVariables>&lt;SharedVariables&gt;&lt;Variables&gt;&lt;/Variables&gt;&lt;/SharedVariables&gt;</SharedVariables>
  <PrimaryCommands></PrimaryCommands>
  <SharedVariableNames/>
  <ClassBinding/>
  <EnabledWorkflows>OnLoadEnabled OnChangeEnabled OnSaveEnabled OnExecuteCommand OnExecuteButton</EnabledWorkflows>
  <SerializedChildrenV2>
    <FormTab xsi:type="DetailTab" flexiWidth="1" flexiHeader="0">
      <Name>General</Name>
      <SerializedChildren>
        <DetailItem SchemaName="Asset Image" image="true" Style="FormNoteImage">
          <IsConstant>true</IsConstant>
          <AutoGridLayout>1</AutoGridLayout>
        </DetailItem>
        <DetailItem SchemaName="msdyn_product"/>
        <DetailItem SchemaName="msdyn_name"/>
        <DetailItem SchemaName="msdyn_account"/>
        <DetailItem SchemaName="msdyn_parentasset">
          <LookupViews>&lt;lookup&gt;&lt;extra/&gt;&lt;extra&gt;&lt;views&gt;msdyn_customerasset.Default&lt;/views&gt;&lt;/extra&gt;&lt;/lookup&gt;</LookupViews>
        </DetailItem>
        <DetailItem SchemaName="msdyn_customerassetcategory"/>
        <DetailItem SchemaName="btc_lastinspected"/>
        <DetailItem SchemaName="btc_installdate"/>
        <DetailItem SchemaName="btc_physicaluid"/>
        <DetailItem SchemaName="ownerid"/>
      </SerializedChildren>
    </FormTab>
    <FormTab xsi:type="IFrameTab">
      <Name>Customer Asset</Name>
      <Url>file://CustomerAsset/CustomerAsset.html</Url>
      <IsVisible>false</IsVisible>
      <DelayLoad>false</DelayLoad>
    </FormTab>
    <FormTab xsi:type="AssociatedTab">
      <Name>Inspections</Name>
      <CustomName>Inspections</CustomName>
      <Binding SchemaName="resco_questionnaire.@virtual"/>
      <Views>&lt;extra&gt;&lt;views&gt;resco_questionnaire.Answered Questionnaires:resco_questionnaire.Associated Questionnaires:resco_questionnaire.Completed This Week:resco_questionnaire.In progress or completed:resco_questionnaire.Reuse&lt;/views&gt;&lt;filter entity="resco_questionnaire"&gt;&amp;lt;fetch&amp;gt;&amp;lt;entity name="resco_questionnaire"&amp;gt;	&amp;lt;filter type="and"&amp;gt;		&amp;lt;condition attribute='resco_istemplate' operator='eq' value='0' /&amp;gt;		&amp;lt;filter type="or"&amp;gt;			&amp;lt;condition attribute="resco_regardingid" operator="eq" value="{{msdyn_customerassetid}}"/&amp;gt;			&amp;lt;condition attribute="resco_rawidvalue" entityname="QuestionLinkAlias" operator="eq" value="{{msdyn_customerassetid}}"/&amp;gt;		&amp;lt;/filter&amp;gt;	&amp;lt;/filter&amp;gt;&amp;lt;link-entity name="resco_question" alias="QuestionLinkAlias" from="resco_questionnaireid" to="resco_questionnaireid" link-type="outer"/&amp;gt;&amp;lt;/entity&amp;gt;&amp;lt;/fetch&amp;gt;&lt;/filter&gt;&lt;/extra&gt;</Views>
      <LookupViews>&lt;extra&gt;&lt;views&gt;resco_questionnaire.Answered Questionnaires:resco_questionnaire.Associated Questionnaires:resco_questionnaire.Questionnaire Templates Lookup:resco_questionnaire.Templates:resco_questionnaire.Reuse&lt;/views&gt;&lt;filter entity="resco_questionnaire"&gt;&amp;lt;fetch&amp;gt;&amp;lt;entity name='resco_questionnaire'&amp;gt;&amp;lt;filter&amp;gt;&amp;lt;condition attribute='resco_istemplate' operator='eq' value='1' /&amp;gt;&amp;lt;/filter&amp;gt;&amp;lt;/entity&amp;gt;&amp;lt;/fetch&amp;gt;&lt;/filter&gt;&lt;/extra&gt;</LookupViews>
    </FormTab>
    <FormTab xsi:type="AssociatedTab" DocCommands="0">
      <Name>Sub Assets</Name>
      <CustomName>Sub Assets</CustomName>
      <Binding SchemaName="msdyn_customerasset.msdyn_masterasset"/>
      <Views>msdyn_customerasset.Default</Views>
    </FormTab>
    <FormTab xsi:type="AssociatedTab">
      <Name>Work Orders List</Name>
      <Binding SchemaName="msdyn_workorder.msdyn_customerasset"/>
    </FormTab>
    <FormTab xsi:type="AssociatedTab">
      <Name>Notes List</Name>
      <Binding SchemaName="annotation.objectid"/>
    </FormTab>
    <FormTab xsi:type="AssociatedTab">
      <Name>Activity List</Name>
      <Binding SchemaName="activity.regardingobjectid"/>
    </FormTab>
  </SerializedChildrenV2>
</EntityForm>